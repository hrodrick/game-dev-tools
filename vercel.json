{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "^/(.*)",
      "dest": "/$1"
    }
  ]
}